      "source": [
        "for num in range(1042000, 702648256):\n",
        "\n",
        "  \n",
        "   order = len(str(num))\n",
        "    \n",
        "\n",
        "   sum = 0\n",
        "\n",
        "   temp = num\n",
        "   while temp > 0:\n",
        "       digit = temp % 10\n",
        "       sum += digit ** order\n",
        "       temp //= 10\n",
        "\n",
        "   if num == sum:\n",
        "       print(num)\n",
        "       break"
      ],
